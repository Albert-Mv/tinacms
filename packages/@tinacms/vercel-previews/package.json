{
  "name": "@tinacms/vercel-previews",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "./dist/index.es.js",
  "keywords": [
    "tinacms",
    "cms",
    "react"
  ],
  "files": [
    "dist"
  ],
